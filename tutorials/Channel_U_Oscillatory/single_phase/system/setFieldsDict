FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    boxToCell
    {
        box (0.15 -0.1 -1) (0.25 0.1 1);
        fieldValues
        (
            volScalarFieldValue eps 0.7
            // volScalarFieldValue K 2.5E-6
        );
    }
);


